{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "devPath": "http://localhost:8000",
    "distDir": "../dist"
  },
  "package": {
    "productName": "RoboMaster UI Designer",
    "version": "1.4.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "readFile": true,
        "writeFile": true
      },
      "dialog": {
        "open": true,
        "save": true
      },
      "window": {
        "maximize": true,
        "unmaximize": true,
        "minimize": true,
        "setDecorations": true,
        "startDragging": true
      },
      "process": {
        "exit": true
      },
      "shell": {
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.bismarck.rmUiDesigner",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://ui.bismarck.xyz/update.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEFCMDQyOEQ2NzA4QTYzNApSV1EwcGdobmpVS3dDdDQ0YnNPamtBQldpakx0aVZ0TlRIMzJPeHJYWGdsZVE5LzFSZmJ1Rnk3Mgo="
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "RoboMaster UI Designer",
        "width": 1280,
        "decorations": false,
        "minHeight": 420,
        "minWidth": 880
      }
    ]
  }
}